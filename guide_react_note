# 創建專案
npm create vite@latest my-react-app -- --template react  

# 進入專案目錄
cd my-react-app  

# 安裝依賴
npm install  

# 啟動開發伺服器
npm run dev  

# 環境
npm install react-router-dom
npm install axios
npm install react-router-dom

# 已有的專案安裝vite
npm install --save-dev vite
npm install react react-dom
npm install --save-dev @vitejs/plugin-react


src/
├── App.jsx
├── components/
│   ├── NavBar.jsx
│   └── NavBar.module.css
└── pages/
    ├── Home.jsx
    └── Home.module.css